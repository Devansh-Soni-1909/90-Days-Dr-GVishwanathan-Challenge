class Solution {
public:
    int findNumbers(vector<int>& nums) {
        int ans = 0;
        for(int i : nums){
            int cnt = 0;
            int t = i;
            while(t){
                cnt++;
                t/=10;

            }

            if(cnt%2==0) ans++;
        }
        return ans;

        
    }
};
